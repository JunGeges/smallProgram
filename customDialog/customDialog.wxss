/* customDialog/customDialog.wxss */
.container {}

.container view {
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
  color: #343434;
  font-size: 30rpx
}

.mask {
  background: #000;
  opacity: 0.4;
  width: 100vw;
  height: 100vh;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 1
}

.dialog {
  width: 690rpx;
  height: 690rpx;
  border-radius: 20rpx;
  background: #fff;
  position: fixed;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  margin: auto;
  z-index: 2;
  overflow: scroll
}

.dialog view {
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
  color: #343434;
  font-size: 30rpx
}

/* .resetContainer {
  overflow: hidden;
